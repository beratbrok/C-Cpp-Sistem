// insertion sort (sokma siralamasi)
// belli bir yere kadar sayilarin sirali oldugunu kabul et
// en sondan basla ve bir yanindaki eleman ile kiyasla
// duruma uymuyorsa bu sekilde devam et.

#include<stdio.h>
#include<time.h>
int main(){
	srand(time(0));
	
	int dizi[10];
	int i=0;
	for( ;i<10;i++)
		dizi[i] = rand()%15;
		
	i = 0;
	for( ;i<10;i++)
		printf("%d ",dizi[i]);
		
	printf("\n");
	// siralama islemini yapalim
	int temp;
	i=0;
	for( ;i<10;i++){
 
		int j=9;
		for( ;j>0;j--){
			if(dizi[j] < dizi[j-1]){
				temp = dizi[j];
				dizi[j] = dizi[j-1];
				dizi[j-1] = temp;
			}
		}
	}
	i = 0;
	for( ;i<10;i++)
		printf("%d ",dizi[i]);
}
