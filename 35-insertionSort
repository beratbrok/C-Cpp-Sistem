// insertion sort (sokma siralamasi)
// belli bir yere kadar sayilarin sirali oldugunu kabul et
// en sondan basla ve bir yanindaki eleman ile kiyasla
// duruma uymuyorsa bu sekilde devam et.

#include<stdio.h>
#include<time.h>
int main(){
	srand(time(0));
	
	int dizi[10];
	int i=0;
	for( ;i<10;i++)
		dizi[i] = rand()%15;
		
	i = 0;
	for( ;i<10;i++)
		printf("%d ",dizi[i]);
		
	printf("\n");
	// siralama islemini yapalim
	int j = 1;
	 for ( ; j < 10; j++)
       {
           int key = dizi[j];
           int i = j - 1;
           while (i >= 0 && dizi[i] > key)
           {
                dizi[i + 1] = dizi[i];
                i = i - 1;
            }
            dizi[i + 1] = key;
       }
	i = 0;
	for( ;i<10;i++)
		printf("%d ",dizi[i]);
}
