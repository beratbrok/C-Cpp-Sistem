 
#include <stdio.h>
#include <stdlib.h>

int main(int argc, char** argv) {

    
    int k = 1;
    
    while(k++<5){
    for(int i=0;i<10;i++){
        for(int j=0;j<10;j++){
            if(i == j) printf(">");
            else printf("*");
        } 
        printf("\n\n");
    }
    for(int i=0;i<10;i++){
        for(int j=9;j>=0;j--){
            if(i==j)printf("<");
            else printf("*");
        }
        printf("\n\n");
    }
    
    }
    
    return (EXIT_SUCCESS);
}
